{
  "template": {
    "ButtonLineEdit": [
      "QPushButton, QLineEdit {",
      "    color: {{ white }};",
      "    background-color: {{ backgroundWidget }};",
      "    border-radius: {{ radius }};",
      "    padding: 0;",
      "    padding-left: {{ padding }};",
      "    padding-right: {{ padding }};",
      "    height: 22;",
      "}",
      "",
      "QPushButton:pressed {",
      "    background-color: {{ primary }};",
      "    color: {{ white }};",
      "}",
      "",
      "QPushButton[primary=true] {",
      "    background-color: {{ primary }};",
      "}",
      "",
      "QPushButton:pressed:hover {",
      "    background-color: {{ primaryHighlighted }};",
      "}",
      "",
      "QPushButton[secondary=true] {",
      "    background-color: transparent;",
      "    border: 2px solid {{ backgroundWidget }};",
      "    height: 20;",
      "}",
      "",
      "QPushButton[secondary=true]:hover {",
      "    background-color: {{ backgroundWidget }};",
      "    border: 2px solid {{ backgroundWidget }};",
      "    height: 20;",
      "}",
      "",
      "QPushButton[secondary=true]:pressed {",
      "    background-color: {{ background }};",
      "}",
      "",
      "QLineEdit:disabled,",
      "QPushButton:disabled,",
      "QPushButton[primary=true]:disabled {",
      "    color: {{ white70 }};",
      "    background-color: {{ backgroundWidget85 }};",
      "    border-color: {{ backgroundLight }};",
      "    border-width: 1px;",
      "    border-style: solid;",
      "}"
    ],
    "Checkbox": [
      "QCheckBox, QRadioButton {",
      "    padding: {{ radius }};",
      "    padding-left: 4px;",
      "}",
      "",
      "QCheckBox:disabled,",
      "QGroupBox::title:disabled,",
      "QRadioButton:disabled {",
      "    color: {{ inactive }};",
      "}",
      "",
      "QGroupBox::indicator::hover,",
      "QCheckBox::hover,",
      "QRadioButton::hover {",
      "    background-color: {{ hover }};",
      "}",
      "",
      "QRadioButton::hover {",
      "    border-radius: 9px;",
      "}",
      "",
      "QCheckBox::indicator,",
      "QGroupBox::indicator,",
      "QRadioButton::indicator {",
      "    border: 1px solid {{ white70 }};",
      "    border-radius: {{ radius }};",
      "}",
      "",
      "QCheckBox::indicator:disabled,",
      "QGroupBox::indicator:disabled,",
      "QRadioButton::indicator:disabled {",
      "    border: 1px solid {{ white50 }};",
      "    border-radius: {{ radius }};",
      "}",
      "",
      "QCheckBox::indicator:checked,",
      "QGroupBox::indicator:checked,",
      "QRadioButton::indicator:checked {",
      "    border: 1px solid {{ primary }};",
      "    background-color: {{ primary }};",
      "}",
      "",
      "QCheckBox::indicator:checked:disabled,",
      "QGroupBox::indicator:checked:disabled,",
      "QRadioButton::indicator:checked:disabled {",
      "    border: 1px solid {{ white50 }};",
      "    background-color: {{ white50 }};",
      "}",
      "",
      "QCheckBox::indicator:checked:hover,",
      "QGroupBox::indicator:checked:hover,",
      "QRadioButton::indicator:checked:hover {",
      "    background-color: {{ primaryHighlighted }};",
      "}",
      "",
      "QRadioButton::indicator,",
      "QRadioButton::indicator:disabled {",
      "   border-radius: 6px;",
      "}"
    ],
    "ComboBox": [
      "QComboBox {",
      "    background-color: {{ backgroundWidget }};",
      "    selection-color: {{ white }};",
      "    selection-background-color: {{ primary }};",
      "    padding: {{ padding }};",
      "    combobox-popup: 0;",
      "    border-bottom-right-radius: {{ radius2 }};",
      "    height: 10px;",
      "}",
      "",
      "QComboBox::drop-down {",
      "    background-color: {{ backgroundLight }};",
      "    border-bottom-left-radius: {{ radius2 }};",
      "    border-bottom-right-radius: {{ radius2 }};",
      "}",
      "",
      "QComboBox::drop-down:hover {",
      "    background-color: {{ backgroundLight }};",
      "}",
      "",
      "QComboBox::hover {",
      "    background-color: {{ hover }};",
      "}",
      "",
      "QComboBox > QListView::item {",
      "    height: {{ comboBoxItemHeight }};",
      "    border: 1px solid white;",
      "}"
    ],
    "Common": [
      "* {",
      "    font: {{ fontSize }};",
      "    background: transparent;",
      "    color: {{ white }};",
      "    selection-color: {{ white }};",
      "    selection-background-color: {{ primary }};",
      "    border-radius: {{ radius }};",
      "    outline: none;",
      "}",
      "",
      "QLineEdit:focus, QPushButton:focus , QSpinBox:focus {",
      "    border-bottom: 2px solid {{ white }};",
      "    border-bottom-left-radius: 0px;",
      "    border-bottom-right-radius: 0px;",
      "}",
      "",
      "QPlainTextEdit:focus {",
      "    border-left: 2px solid {{ white }};",
      "    border-top-left-radius: 0px;",
      "    border-bottom-left-radius: 0px;",
      "}",
      "",
      "QLineEdit:hover,",
      "QPushButton:hover,",
      "QHeaderView::section:hover, ",
      "QSpinBox:hover {",
      "    background-color: {{ hover }};",
      "    color: {{ white }};",
      "}",
      "",
      "QPushButton:checked {",
      "    background-color: {{ primary }};",
      "}",
      "",
      "QPushButton:disabled:checked {",
      "    background-color: {{ backgroundLight }};",
      "}",
      "",
      "QPlainTextEdit:active,",
      "QPlainTextEdit:!active  {",
      "    background-color: {{ transparentWhite }};",
      "}",
      "",
      "QPlainTextEdit[readOnly=\"true\"],",
      "QPlainTextEdit[readOnly=\"true\"]:focus {",
      "    border: None;",
      "}",
      "",
      "QToolTip {",
      "    color: {{ white }};",
      "    border: 1px solid {{ hover }};",
      "    padding: {{ radius }};",
      "    background-color: {{ background }};",
      "}"
    ],
    "Dialog": [
      "QDialog {",
      "    background-color: {{ background }};",
      "}",
      "",
      "QDialog > QLabel {",
      "    background-color: {{ background }};",
      "}"
    ],
    "Dock": [
      "QDockWidget {",
      "    background: {{ backgroundFrame }};",
      "}",
      "",
      "QDockWidget::title {",
      "    background: {{ backgroundFrame }};",
      "    border-bottom: 1px solid {{ backgroundLight }};",
      "    border-bottom-left-radius: 0px;",
      "    border-bottom-right-radius: 0px;",
      "    margin-top: 5px;",
      "    margin-right: 50px;",
      "}",
      "",
      "QDockWidget::close-button, QDockWidget::float-button {",
      "    color: {{ white }};",
      "    background: {{ backgroundWidget }};",
      "    margin: {{ radius2 }};",
      "    border-radius: {{ radius }};",
      "}",
      "",
      "QDockWidget::close-button:hover, QDockWidget::float-button:hover {",
      "    color: {{ white }};",
      "    background: {{ hover }};",
      "}",
      "",
      "QDockWidget::close-button:pressed, QDockWidget::float-button:pressed {",
      "    background: {{ primary }};",
      "}"
    ],
    "Frame": [
      "QFrame {",
      "    background-color: {{ backgroundFrame }};",
      "}"
    ],
    "Groups": [
      "QGroupBox {",
      "  background-color: {{ transparentWhite }};",
      "  border: none;",
      "  margin-top: 9px;",
      "  padding-top: {{ padding }};",
      "}",
      "",
      "QGroupBox::title {",
      "    top: -10px;",
      "    left: 10px;",
      "    color: {{ white }};",
      "    background: transparent;",
      "   padding: {{ radius }};",
      "}"
    ],
    "Label": [
      "QLabel {",
      "    background-color: transparent;",
      "    color: {{ white80 }};",
      "    padding: {{ radius }};",
      "}",
      "",
      "QLabel:disabled {",
      "    color: {{ inactive }};",
      "}",
      "",
      "QLabel[isWhite=\"true\"] {",
      "    color: {{ white }};",
      "}"
    ],
    "Main Window": [
      "QMainWindow {",
      "    background-color: {{ background }};",
      "}"
    ],
    "Menus": [
      "QMenu {",
      "    background-color: {{ backgroundFrame }};",
      "    padding: {{ radius }};",
      "    border: 1px solid {{ backgroundWidget }};",
      "    border-radius: 0px;",
      "}",
      "",
      "QMenu::item {",
      "    color: {{ white }};",
      "    background-color: transparent;",
      "}",
      "",
      "QMenu::item:selected {",
      "    background-color: {{ backgroundWidget }};",
      "}",
      "",
      "QMenu::item:checked {",
      "    background-color: {{ primary }};",
      "}",
      "",
      "QMenu::item:checked:selected {",
      "    background-color: {{ primaryHighlighted }};",
      "}",
      "",
      "",
      "QMenu::item:disabled {",
      "    background-color: {{ transparentWhite }};",
      "    color: {{ backgroundLight }};",
      "}",
      "",
      "",
      "QMenu::separator {",
      "    height: 2px;",
      "    background: {{ backgroundWidget }};",
      "    margin-left: 10px;",
      "    margin-right: 5px;",
      "}",
      "",
      "",
      "QMenuBar {",
      "    background-color: {{ transparentWhite }};",
      "    spacing: {{ padding }};",
      "}",
      "",
      "QMenuBar:disabled {",
      "    color:  {{ inactive }};",
      "}",
      "",
      "QMenuBar::item {",
      "    padding: {{ padding }};",
      "    border-radius: 0px;",
      "}",
      "",
      "QMenuBar::item:selected {",
      "    background-color: {{ primary }};",
      "    padding: {{ padding }};",
      "    border-radius: 0px;",
      "}",
      "",
      "QMenuBar::item:pressed {",
      "    background: {{ backgroundLight }};",
      "}"
    ],
    "Progress": [
      "QProgressBar {",
      "    border: none;",
      "    background-color: {{ backgroundWidget }};",
      "    padding: 0;",
      "    border-radius: {{ radius }};",
      "}",
      "",
      "QProgressBar:horizontal {",
      "    margin-top: {{ padding }};",
      "    margin-bottom: {{ padding }};",
      "    height: 0;",
      "}",
      "",
      "QProgressBar:vertical {",
      "    width: 0;",
      "    margin-left: 7px;",
      "    margin-right: 7px;",
      "}",
      "",
      "QProgressBar::chunk {",
      "    background-color: {{ primary }};",
      "}"
    ],
    "Properties": [
      "QLabel[isTitle=\"true\"] {",
      "    font-size: 11pt;",
      "}",
      "",
      "*[sideStroked=\"true\"], QLabel[isTitle=\"true\"] {",
      "    border-style: solid;",
      "    border-color: {{ primary }};",
      "    border-left-width: {{ padding }};",
      "    border-radius: 0px;",
      "}",
      "",
      "QGroupBox[untitledGroup=\"true\"] {",
      "      padding-top: 0;",
      "}"
    ],
    "QGraphics": [
      "QGraphicsView {",
      "    background: {{ transparentWhite }};",
      "}"
    ],
    "Scroll": [
      "QScrollArea, *[hasBackground=\"true\"] {",
      "    background-color: transparent;",
      "    border-radius: {{ radius }};",
      "}",
      "",
      "QScrollBar {",
      "    background-color: {{ backgroundLight }};",
      "    border-radius: {{ radius }};",
      "    border: none;",
      "}",
      "",
      "QScrollBar:vertical {",
      "    width: {{ scrollbar }};",
      "    margin: 0px 0px 0px {{ padding }};",
      "}",
      "",
      "QScrollBar:horizontal {",
      "    height: {{ scrollbar }};",
      "    margin: {{ padding }} 0px 0px 0px;",
      "}",
      "",
      "QScrollBar::add-page, QScrollBar::sub-page {",
      "    border-radius: {{ radius }};",
      "    background: none;",
      "}",
      "",
      "QScrollBar::handle {",
      "    background-color: {{ primary}};",
      "    min-height: 15px;",
      "    border-radius: {{ radius }};",
      "}",
      "",
      "QScrollBar::add-line, QScrollBar::sub-line {",
      "    border: none;",
      "    background: none;",
      "    width: 0px;",
      "    height: 0px;",
      "}",
      "",
      "QScrollBar::handle:hover {",
      "    background-color: {{ primaryHighlighted }};",
      "}",
      "",
      "QScrollBar::handle:disabled {",
      "    background-color: {{ white80 }};",
      "}"
    ],
    "Slider": [
      "QSlider:horizontal {",
      "    height: 20px;",
      "}",
      "",
      "QSlider:vertical{",
      "    width: 20px;",
      "}",
      "",
      "QSlider::groove {",
      "    background: {{ transparentWhite }};",
      "    position: absolute;",
      "}",
      "",
      "QSlider::groove:focus {",
      "    background: {{ backgroundWidget }};",
      "}",
      "",
      "QSlider::groove:horizontal {",
      "    left: 8px;",
      "    right: 8px;",
      "}",
      "",
      "QSlider::groove:vertical {",
      "    top: 8px;",
      "    bottom: 8px;",
      "}",
      "",
      "QSlider::handle {",
      "    background: {{ primary }};",
      "    border-radius: 7px;",
      "}",
      "",
      "QSlider::handle:hover {",
      "    background: {{ primaryHighlighted }};",
      "}",
      "",
      "QSlider::handle:horizontal {",
      "    margin: -4px -8px; /* expand outside the groove */",
      "}",
      "",
      "QSlider::handle:vertical {",
      "    margin: -8px -4px; /* expand outside the groove */",
      "}",
      "",
      "QSlider::groove:horizontal {",
      "    height: {{ padding }};",
      "}",
      "",
      "QSlider::groove:vertical {",
      "    width: {{ padding }};",
      "}"
    ],
    "SpinBox": [
      "QDoubleSpinBox, ",
      "QSpinBox {",
      "    height: 22px;",
      "    background-color: {{ backgroundWidget }};",
      "    padding-left: 4px;",
      "    border-top-right-radius: {{ radius2 }};",
      "    border-bottom-right-radius: {{ radius2 }};",
      "    border: 2px solid {{ backgroundWidget }};",
      "    height: 20;",
      "}",
      "",
      "QDoubleSpinBox:disabled, ",
      "QSpinBox:disabled {",
      "    color: {{ inactive }}; ",
      "}",
      "",
      "QDoubleSpinBox::up-button, QDoubleSpinBox::down-button,",
      "QSpinBox::up-button, QSpinBox::down-button {",
      "    background-color: {{ backgroundLight }};",
      "    width: 12px;",
      "    height: 8px;",
      "    margin: 1px;",
      "}",
      "",
      "QDoubleSpinBox::up-button, ",
      "QSpinBox::up-button {",
      "    border-top-left-radius: {{ radius2 }};",
      "    border-top-right-radius: {{ radius2 }};",
      "}",
      "",
      "QDoubleSpinBox::down-button, ",
      "QSpinBox::down-button {",
      "    border-bottom-left-radius: {{ radius2 }};",
      "    border-bottom-right-radius: {{ radius2 }};",
      "}",
      "",
      "QDoubleSpinBox::up-button:hover, QDoubleSpinBox::down-button:hover,",
      "QSpinBox::up-button:hover, QSpinBox::down-button:hover {",
      "    background-color: {{ hover }};",
      "}",
      "",
      "QDoubleSpinBox::up-button:pressed, QDoubleSpinBox::down-button:pressed,",
      "QSpinBox::up-button:pressed, QSpinBox::down-button:pressed {",
      "    background-color: {{ primary }};",
      "}"
    ],
    "Status Bar": [
      "QStatusBar {",
      "    background-color: {{ transparentWhite }};",
      "}",
      "",
      "",
      "QStatusBar > QLabel {",
      "    color: {{ white80 }};",
      "    padding: 1;",
      "}",
      "",
      "QStatusBar::item {",
      "    border: none;",
      "}"
    ],
    "Tab": [
      "QTabWidget::pane {",
      "    background-color: transparent;",
      "    border-radius: {{ radius }};",
      "}",
      "",
      "QTabBar {",
      "    background-color: transparent;",
      "}",
      "",
      "QTabBar::tab {",
      "    color: {{ white }};",
      "    background-color: {{ backgroundWidget }};",
      "    padding: 1px;",
      "}",
      "",
      "QTabBar::tab:top,",
      "QTabBar::tab:bottom {",
      "    padding-left: {{ padding }};",
      "    padding-right: {{ padding }};",
      "    height: 22;",
      "    margin-left: 1px;",
      "    margin-right: 1px;",
      "}",
      "",
      "QTabBar::tab:top {",
      "    border-top-left-radius: {{ radius }};",
      "    border-top-right-radius: {{ radius2 }};",
      "}",
      "",
      "QTabBar::tab:bottom {",
      "    border-bottom-left-radius: {{ radius2 }};",
      "    border-bottom-right-radius: {{ radius }};",
      "}",
      "",
      "QTabBar::tab:left,",
      "QTabBar::tab:right {",
      "    padding-top: {{ padding }};",
      "    padding-bottom: {{ padding }};",
      "    width: 22;",
      "    margin-top: 1px;",
      "    margin-bottom: 1px;",
      "}",
      "",
      "QTabBar::tab:left {",
      "    border-top-left-radius: {{ radius }};",
      "    border-bottom-left-radius: {{ radius2 }};",
      "}",
      "",
      "QTabBar::tab:right {",
      "    border-top-right-radius: {{ radius }};",
      "    border-bottom-right-radius: {{ radius2 }};",
      "}",
      "",
      "QTabBar::tab:hover {",
      "    background-color: {{ hover }};",
      "    color: {{ white }};",
      "}",
      "",
      "QTabBar::tab:pressed {",
      "    background-color: {{ primary }};",
      "    color: {{ white }};",
      "}",
      "",
      "QTabBar::tab:selected {",
      "    background-color: {{ primary }};",
      "    color: {{ white }};",
      "}",
      "",
      "QTabBar::tab:selected:left,",
      "QTabBar::tab:selected:right {",
      "    border-bottom: 2px solid {{ white }};",
      "    margin-top: 1px;",
      "}",
      "",
      "QTabBar::tab:selected:right {",
      "    border-bottom-right-radius: {{ radius }};",
      "}",
      "",
      "QTabBar::tab:selected:left {",
      "    border-bottom-left-radius: {{ radius }};",
      "}",
      "",
      "QTabBar::tab:selected:top,",
      "QTabBar::tab:selected:bottom {",
      "    border-left: 2px solid {{ white }};",
      "    margin-left: 1px;",
      "}",
      "",
      "QTabBar::tab:selected:top {",
      "    border-top-right-radius: {{ radius }};",
      "}",
      "",
      "QTabBar::tab:selected:bottom {",
      "    border-bottom-left-radius: {{ radius }};",
      "}",
      "",
      "QTabBar::tab:disabled {",
      "    color: {{ white60 }};",
      "}",
      "",
      "QTabBar::tab:selected:disabled {",
      "    background-color: {{ backgroundLight }};",
      "    color: {{ white40 }};",
      "}"
    ],
    "Tables, Trees": [
      "QAbstractItemView {",
      "    color: {{ white }};",
      "    background-color: {{ backgroundWidget }};",
      "    alternate-background-color: {{ backgroundWidget90 }};",
      "}",
      "",
      "QAbstractItemView > QWidget > QLineEdit,",
      "QAbstractItemView > QWidget > QLineEdit:hover {",
      "    background: {{ primary }};",
      "    border: none;",
      "    border-bottom: 2px solid {{ white }};",
      "    border-radius: 0px;",
      "    selection-color: {{ primary }};",
      "    selection-background-color: {{ white }};",
      "}",
      "",
      "QAbstractItemView::item:focus,",
      "QAbstractItemView::item:selected",
      "{",
      "    border: none;",
      "    padding-left: 2px;",
      "}",
      "",
      "QAbstractItemView::item:focus",
      "{",
      "    border-bottom: 2px solid {{ white }};",
      "    padding-bottom: -1px;",
      "}",
      "",
      "QListView {",
      "    background-color: {{ backgroundLight }};",
      "}",
      "",
      "QWidget > QListView {",
      "    background: transparent;",
      "}",
      "",
      "QListView::item:focus",
      "{",
      "    background: transparent;",
      "    border: none;",
      "}",
      "",
      "QAbstractItemView::item:selected:active {",
      "    color: {{ white }};",
      "    background-color: {{ primary }};",
      "}",
      "",
      "QAbstractItemView::item:selected:!active {",
      "    color: {{ background }};",
      "    background-color: {{ inactive }};",
      "}",
      "",
      "QHeaderView::section {",
      "    color: {{ white }};",
      "    background-color: {{ backgroundFrame }};",
      "    border-color: {{ backgroundFrame }};",
      "    border-style: solid;",
      "    border-left-width: 1 px;",
      "    border-right-width: 1 px;",
      "    border-top-width: 1 px;",
      "    border-bottom-width: 1 px;",
      "    padding: {{ radius }};",
      "}",
      "",
      "QAbstractItemView QTableCornerButton::section {",
      "    border-color: {{ backgroundFrame }};",
      "    border-style: solid;",
      "    border-left-width: 1 px;",
      "    border-right-width: 1 px;",
      "    border-top-width: 1 px;",
      "    border-bottom-width: 1 px;",
      "    background-color: {{ backgroundFrame }};",
      "}",
      "",
      "QHeaderView::section:hover {",
      "    background-color: {{ hover }};",
      "    color: {{ white }};",
      "}",
      "",
      "",
      "QAbstractItemView:disabled,",
      "QHeaderView::section:disabled {",
      "    color: {{ white70 }};",
      "}"
    ],
    "Toolbar": [
      "QToolBar  {",
      "    background-color: {{ transparentWhite }};",
      "    padding: {{ radius }};",
      "}",
      "",
      "QToolBar::separator {",
      "    background-color: {{ backgroundLight }};",
      "    margin: 2px;",
      "}",
      "",
      "QToolButton, QToolButton:unchecked {",
      "    background: transparent;",
      "    border-radius: {{ radius }};",
      "}",
      "",
      "QToolButton:hover {",
      "    background-color: {{ hover }}",
      "}",
      "",
      "QToolButton:pressed {",
      "    background-color: {{ primary }};",
      "}",
      "",
      "QToolButton:checked {",
      "    background-color: {{ primary }};",
      "}",
      "",
      "QToolButton:checked:hover {",
      "    background-color: {{ primaryHighlighted }};",
      "}",
      "",
      "QToolBar::handle {",
      "    background-color: {{ transparentWhite }};",
      "}",
      "",
      "QToolBar::handle:horizontal {",
      "    margin-right: 1px;",
      "}",
      "",
      "QToolBar::handle:vertical {",
      "    margin-bottom: 1px;",
      "}"
    ]
  },
  "variables": {
    "background": [
      66,
      66,
      66
    ],
    "backgroundFrame": [
      66,
      66,
      66
    ],
    "backgroundLight": [
      140,
      140,
