{
  "version": 1,
  "templates": {
    "Main": [
      "* {",
      "  font: {{ font_size }}pt;",
      "  color: {{ foreground }};",
      "  background-color: {{ background }};",
      "  border-radius: {{ radius }}px;",
      "  outline: none;",
      "  padding: {{ padding }}px;",
      "}",
      "",
      "{{ interactives }} {",
      "  background-color: {{ background_layered }};",
      "}",
      "",
      "{{ interactives }}:focus {",
      "  border: {{ line_size }}px solid {{ foreground }};",
      "}",
      "",
      "{{ interactives }}:hover {",
      "  background-color: {{ hovered }};",
      "}",
      "",
      "{{ interactives }}:pressed {",
      "  background-color: {{ foreground }};",
      "  color: {{ background }};",
      "}",
      "",
      "{{ interactives }}:selected {",
      "  background-color: {{ foreground }};",
      "  color: {{ background }};",
      "}",
      "",
      "",
      "{{ checkables }}:checked {",
      "  background-color: {{ foreground }};",
      "  color: {{ background }};",
      "}",
      "",
      "QMainWindow {",
      "  background-color: {{ background }};",
      "}",
      "",
      "QLabel {",
      "  background-color: transparent;",
      "}",
      "",
      "QPushButton[accent=\"1\"] {",
      "  background-color: {{ accent1 }};",
      "}",
      "",
      "QPushButton[accent=\"1\"]:hover {",
      "  background-color: {{ accent1_hover }};",
      "}",
      "",
      "QPushButton[accent=\"1\"]:pressed {",
      "  background-color: {{ accent1_pressed }};",
      "  color: {{ accent1 }};",
      "}",
      "",
      "QTabBar::tab {",
      "    border: none;",
      "    padding: {{ padding }}px;",
      "}",
      "",
      "QToolBar  {",
      "    background-color: {{ background }};",
      "}"
    ]
  },
  "variables": [
    {
      "name": "background",
      "desktop_value": "[70, 70, 70]",
      "touch_value": ""
    },
    {
      "name": "foreground",
      "desktop_value": "[252, 252, 252]",
      "touch_value": ""
    },
    {
      "name": "accent1_hover",
      "desktop_value": "[99, 199, 255]",
      "touch_value": ""
    },
    {
      "name": "padding",
      "desktop_value": "2",
      "touch_value": "10"
    },
    {
      "name": "font_size",
      "desktop_value": "10",
      "touch_value": "12"
    },
    {
      "name": "background_layered",
      "desktop_value": "[255, 255, 255, 30]",
      "touch_value": ""
    },
    {
      "name": "radius",
      "desktop_value": "2",
      "touch_value": "4"
    },
    {
      "name": "line_size",
      "desktop_value": "1",
      "touch_value": "2"
    },
    {
      "name": "interactives",
      "desktop_value": "{QLineEdit, QTextEdit, QPlainTextEdit, QSpinBox, QDoubleSpinBox, QDateEdit, QTimeEdit, QDateTimeEdit, QComboBox, QPushButton, QCheckBox, QRadioButton, QTabBar::tab}",
      "touch_value": ""
    },
    {
      "name": "hovered",
      "desktop_value": "[153, 153, 153]",
      "touch_value": ""
    },
    {
      "name": "accent1",
      "desktop_value": "[84, 165, 211]",
      "touch_value": ""
    },
    {
      "name": "accent1_pressed",
      "desktop_value": "[234, 245, 255]",
      "touch_value": ""
    },
    {
      "name": "checkables",
      "desktop_value": "{QPushButton}",
      "touch_value": ""
    }
  ],
  "save_to": "E:\\PROJECTS_2023\\pyside6-helpers\\pyside6helpers\\resources\\dark-2.qss"
}