{
  "version": 1,
  "templates": {
    "Main": [
      "* {",
      "  font: {{ font_size }}pt;",
      "  color: {{ foreground }};",
      "  background-color: {{ background }};",
      "  border-radius: {{ radius }}px;",
      "  outline: none;",
      "  padding: {{ padding }}px;",
      "}",
      "",
      "QMainWindow {",
      "  background-color: {{ background }};",
      "}",
      "",
      "QLabel {",
      "  background-color: transparent;",
      "}",
      "",
      "",
      "QToolBar  {",
      "  background-color: {{ background_layered }};",
      "}"
    ],
    "Buttons/Inputs": [
      "{{ interactives }} {",
      "  background-color: {{ background_layered }};",
      "}",
      "",
      "{{ interactives }}:focus {",
      "  border: {{ line_size }}px solid {{ focus_border }};",
      "}",
      "",
      "{{ interactives }}:hover {",
      "  background-color: {{ hovered }};",
      "}",
      "",
      "{{ pressables }}:pressed {",
      "  background-color: {{ foreground }};",
      "  color: {{ background }};",
      "}",
      "",
      "{{ interactives }}:selected {",
      "  background-color: {{ foreground }};",
      "  color: {{ background }};",
      "}",
      "",
      "",
      "{{ checkables }}:checked {",
      "  background-color: {{ foreground }};",
      "  color: {{ background }};",
      "}",
      "",
      "{{ interactives }}:disabled {",
      "  border: {{ line_size }}px  solid {{ disabled_foreground }};",
      "  color: {{ disabled_foreground }};",
      "}",
      "",
      "{{ checkables }}:disabled:checked {",
      "  background: {{ disabled_foreground }};",
      "  border: {{ line_size }}px  solid {{ disabled_foreground }};",
      "  color: {{ foreground }};",
      "}",
      "",
      "{{ interactives }}:disabled:selected {",
      "  background: {{ disabled_foreground }};",
      "  border: {{ line_size }}px  solid {{ disabled_foreground }};",
      "  color: {{ foreground }};",
      "}",
      "",
      "{{ indicators }} {",
      "  border: {{ line_size }}px solid {{ disabled_foreground }};",
      "}",
      "",
      "{{ indicators }}:checked {",
      "  background-color: {{ accent1 }};",
      "  border: {{ accent1_hover }};",
      "}",
      "",
      "{{ indicators }}:indeterminate {",
      "  background-color: {{ accent1_pressed }};",
      "  border: {{ accent1_hover }};",
      "}",
      "",
      "{{ hoverables_accent }} {",
      "  background-color: {{ accent1 }};",
      "}",
      "",
      "{{ hoverables_accent }}:hover {",
      "  background-color: {{ accent1_hover }};",
      "}",
      "",
      "QPushButton[accent=\"1\"]:pressed,",
      "QPushButton[accent=\"1\"]:checked {",
      "  background-color: {{ accent1_pressed }};",
      "  color: {{ accent1 }};",
      "}",
      "",
      "QCheckBox, QRadioButton {",
      "  padding-left: {{ padding_top }}px;",
      "}",
      "",
      "QRadioButton::indicator {",
      "  border-radius: 6px",
      "}",
      "",
      "QRadioButton {",
      "  border-radius: {{ round_radius }}px;",
      "}"
    ],
    "Containers": [
      "/* Tab Bar */",
      "QTabWidget::pane {",
      "  background-color: transparent;",
      "  border-radius: 2px;",
      "}",
      "",
      "QTabWidget>QFrame {",
      "  background-color: {{ background_layered }};",
      "}",
      "",
      "QTabBar::tab {",
      "  border: none;",
      "  padding: {{ padding }}px;",
      "}",
      "",
      "QTabBar::tab:top,",
      "QTabBar::tab:bottom {",
      "  padding-left: {{ padding_top }}px;",
      "  padding-right: {{ padding_top }}px;",
      "  height: 22;",
      "  margin-left: 1px;",
      "  margin-right: 1px;",
      "}",
      "",
      "QTabBar::tab:top {",
      "  border-top-left-radius: {{ radius }}px;",
      "  border-top-right-radius: {{ padding_top }}px;",
      "}",
      "",
      "QTabBar::tab:bottom {",
      "  border-bottom-left-radius: {{ padding_top }}px;",
      "  border-bottom-right-radius: {{ radius }}px;",
      "}",
      "",
      "QTabBar::tab:left,",
      "QTabBar::tab:right {",
      "  padding-top: {{ padding_top }}px;",
      "  padding-bottom: {{ padding_top }}px;",
      "  width: 22;",
      "  margin-top: 1px;",
      "  margin-bottom: 1px;",
      "}",
      "",
      "QTabBar::tab:left {",
      "    border-top-left-radius: {{ radius }}px;",
      "    border-bottom-left-radius: {{ padding_top }}px;",
      "}",
      "",
      "QTabBar::tab:right {",
      "  border-top-right-radius: {{ radius }}px;",
      "  border-bottom-right-radius: {{ padding_top }}px;",
      "}",
      "",
      "/* Group Box */",
      "QGroupBox {",
      "  margin-top: {{ padding_top }}px;",
      "  padding-top: {{ padding_top }}px;",
      "  background-color: {{ background_layered }};",
      "}",
      "",
      "QGroupBox::title {",
      "  top: -{{ padding_top }}px;",
      "  left: {{ padding_top }}px;",
      "}",
      "",
      "/* Splitter */",
      "QSplitter::handle {",
      "  background-color: {{ background_layered }};",
      "  margin: {{ padding }}px;",
      "}",
      "",
      "QSplitter>QFrame {",
      "  background-color: {{ background_layered }};",
      "}"
    ],
    "Views": [
      "QAbstractItemView, QListView {",
      "    background-color: {{ views_background }};",
      "    alternate-background-color: {{ views_background_alt }};",
      "}",
      "",
      "QAbstractItemView > QWidget > QLineEdit,",
      "QAbstractItemView > QWidget > QLineEdit:hover {",
      "    background: rgb(75, 165, 193);",
      "    border: none;",
      "    border-bottom: 2px solid rgb(232, 232, 232);",
      "    border-radius: 0px;",
      "    selection-color: rgb(75, 165, 193);",
      "    selection-background-color: rgb(232, 232, 232);",
      "}",
      "",
      "QAbstractItemView::item:focus,",
      "QAbstractItemView::item:selected",
      "{",
      "    border: none;",
      "    padding-left: 2px;",
      "}",
      "",
      "QAbstractItemView::item:focus",
      "{",
      "    padding-bottom: -1px;",
      "}",
      "",
      "QWidget > QListView {",
      "    background: transparent;",
      "}",
      "",
      "QListView::item:focus",
      "{",
      "    background: transparent;",
      "    border: none;",
      "}",
      "",
      "QAbstractItemView::item:selected:active {",
      "    color: rgb(232, 232, 232);",
      "    background-color: rgb(75, 165, 193);",
      "}",
      "",
      "QAbstractItemView::item:selected:!active {",
      "    color: rgb(66, 66, 66);",
      "    background-color: rgb(135, 135, 135);",
      "}",
      "",
      "QHeaderView::section {",
      "    color: rgb(232, 232, 232);",
      "    background-color: rgb(66, 66, 66);",
      "    border-color: rgb(66, 66, 66);",
      "    border-style: solid;",
      "    border-left-width: 1 px;",
      "    border-right-width: 1 px;",
      "    border-top-width: 1 px;",
      "    border-bottom-width: 1 px;",
      "    padding: 2px;",
      "}",
      "",
      "QAbstractItemView QTableCornerButton::section {",
      "    border: none;",
      "    background-color: {{ views_background }};",
      "}",
      "",
      "QHeaderView::section:hover {",
      "    background-color: rgb(117, 117, 117);",
      "    color: rgb(232, 232, 232);",
      "}",
      "",
      "",
      "QAbstractItemView:disabled,",
      "QHeaderView::section:disabled {",
      "    color: rgb(162, 162, 162);",
      "}"
    ],
    "Display": [
      "/* Progress Bar */",
      "QProgressBar {",
      "  border: none;",
      "  background-color: {{ background_layered }};",
      "  padding: 0;",
      "  text-align: center;",
      "}",
      "",
      "QProgressBar:horizontal {",
      "  margin-top: 7px;",
      "  margin-bottom: 7px;",
      "  height: 0;",
      "}",
      "",
      "QProgressBar:vertical {",
      "  width: 0;",
      "  margin-left: 7px;",
      "  margin-right: 7px;",
      "}",
      "",
      "QProgressBar::chunk {",
      "  background-color: {{ accent1 }};",
      "}",
      "",
      "QProgressBar::chunk:disabled {",
      "  background-color: {{ background_layered }};",
      "}",
      "",
      "/* Scroll Bar */",
      "QScrollBar {",
      "  background-color: {{ background_layered }};",
      "  border-radius: 2px;",
      "  border: none;",
      "  padding: 0;",
      "}",
      "",
      "QScrollBar:vertical {",
      "  width: {{ scrollbar_size }}px;",
      "}",
      "",
      "QScrollBar:horizontal {",
      "  height: {{ scrollbar_size }}px;",
      "}",
      "",
      "QScrollBar::add-page, QScrollBar::sub-page {",
      "  border-radius: 2px;",
      "  background: none;",
      "}",
      "",
      "QScrollBar::handle {",
      "  background-color: {{ accent1 }};",
      "  min-height: 15px;",
      "  border-radius: 2px;",
      "}",
      "",
      "QScrollBar::add-line, QScrollBar::sub-line {",
      "  border: none;",
      "  background: none;",
      "  width: 0px;",
      "  height: 0px;",
      "}",
      "",
      "QScrollBar::handle:hover {",
      "  background-color: {{ accent1_hover }};",
      "}",
      "",
      "QScrollBar::handle:disabled {",
      "  background-color: {{ background_layered }};",
      "}",
      "",
      "/* Slider */",
      "QSlider {",
      "  background: none",
      "}",
      "",
      "QSlider::handle {",
      "    border-radius: {{ slider_radius }}px;",
      "}",
      "",
      "QSlider::handle:vertical {",
      "    margin: {{ slider_margin_v }}px {{ slider_margin_h }}px;",
      "}",
      "",
      "QSlider::handle:horizontal {",
      "    margin: {{ slider_margin_h }}px {{ slider_margin_v }}px;",
      "}",
      "",
      "QSlider::groove {",
      "  background: {{ background_layered }};",
      "  position: absolute;",
      "  border-radius: 3px;",
      "}",
      "",
      "QSlider::groove:horizontal {",
      "  height: {{ slider_radius }}px;",
      "  left: 8px;",
      "  right: 8px;",
      "}",
      "",
      "QSlider::groove:vertical {",
      "  width: {{ slider_radius }}px;",
      "  top: 8px;",
      "  bottom: 8px;",
      "}",
      "",
      "QSlider::groove:focus {",
      "    background: {{ hovered }};",
      "}"
    ],
    "Menus": [
      "QMenu {",
      "    background-color: rgb(66, 66, 66);",
      "    padding: 2px;",
      "    border: 1px solid rgb(104, 104, 104);",
      "    border-radius: 0px;",
      "}",
      "",
      "QMenu::item {",
      "    color: rgb(232, 232, 232);",
      "    background-color: transparent;",
      "}",
      "",
      "QMenu::item:selected {",
      "    background-color: rgb(104, 104, 104);",
      "}",
      "",
      "QMenu::item:checked {",
      "    background-color: rgb(75, 165, 193);",
      "}",
      "",
      "QMenu::item:checked:selected {",
      "    background-color: rgb(106, 198, 237);",
      "}",
      "",
      "",
      "QMenu::item:disabled {",
      "    background-color: rgba(255, 255, 255, 15);",
      "    color: rgb(140, 140, 140);",
      "}",
      "",
      "",
      "QMenu::separator {",
      "    height: 2px;",
      "    background: rgb(104, 104, 104);",
      "    margin-left: 10px;",
      "    margin-right: 5px;",
      "}",
      "",
      "",
      "QMenuBar {",
      "    background-color: rgba(255, 255, 255, 15);",
      "    spacing: 7px;",
      "}",
      "",
      "QMenuBar:disabled {",
      "    color:  rgb(135, 135, 135);",
      "}",
      "",
      "QMenuBar::item {",
      "    padding: 7px;",
      "    border-radius: 0px;",
      "}",
      "",
      "QMenuBar::item:selected {",
      "    background-color: rgb(75, 165, 193);",
      "    padding: 7px;",
      "    border-radius: 0px;",
      "}",
      "",
      "QMenuBar::item:pressed {",
      "    background: rgb(140, 140, 140);",
      "}"
    ]
  },
