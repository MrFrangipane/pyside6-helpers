{
  "version": 1,
  "templates": {
    "Main": [
      "* {",
      "  font: {{ font_size }}pt;",
      "  color: {{ foreground }};",
      "  background-color: {{ background }};",
      "  border-radius: {{ radius }}px;",
      "  outline: none;",
      "  padding: {{ padding }}px;",
      "}",
      "",
      "QMainWindow {",
      "  background-color: {{ background }};",
      "}",
      "",
      "QLabel {",
      "  background-color: transparent;",
      "}",
      "",
      "",
      "QToolBar  {",
      "  background-color: {{ background_layered }};",
      "}"
    ],
    "Buttons": [
      "{{ interactives }} {",
      "  background-color: {{ background_layered }};",
      "}",
      "",
      "{{ interactives }}:focus {",
      "  border: {{ line_size }}px solid {{ foreground }};",
      "}",
      "",
      "{{ interactives }}:hover {",
      "  background-color: {{ hovered }};",
      "}",
      "",
      "{{ interactives }}:pressed {",
      "  background-color: {{ foreground }};",
      "  color: {{ background }};",
      "}",
      "",
      "{{ interactives }}:selected {",
      "  background-color: {{ foreground }};",
      "  color: {{ background }};",
      "}",
      "",
      "",
      "{{ checkables }}:checked {",
      "  background-color: {{ foreground }};",
      "  color: {{ background }};",
      "}",
      "",
      "{{ interactives }}:disabled {",
      "  background: none;",
      "  border: {{ line_size }}px  solid {{ disabled_foreground }};",
      "  color: {{ disabled_foreground }};",
      "}",
      "",
      "{{ checkables }}:disabled:checked {",
      "  background: {{ disabled_foreground }};",
      "  border: {{ line_size }}px  solid {{ disabled_foreground }};",
      "  color: {{ foreground }};",
      "}",
      "",
      "{{ interactives }}:disabled:selected {",
      "  background: {{ disabled_foreground }};",
      "  border: {{ line_size }}px  solid {{ disabled_foreground }};",
      "  color: {{ foreground }};",
      "}",
      "",
      "{{ indicators }} {",
      "  border: {{ line_size }}px solid {{ disabled_foreground }};",
      "}",
      "",
      "{{ indicators }}:checked {",
      "  background-color: {{ accent1 }};",
      "  border: {{ accent1_hover }};",
      "}",
      "",
      "{{ indicators }}:indeterminate {",
      "  background-color: {{ accent1_pressed }};",
      "  border: {{ accent1_hover }};",
      "}",
      "",
      "QPushButton[accent=\"1\"] {",
      "  background-color: {{ accent1 }};",
      "}",
      "",
      "QPushButton[accent=\"1\"]:hover {",
      "  background-color: {{ accent1_hover }};",
      "}",
      "",
      "QPushButton[accent=\"1\"]:pressed,",
      "QPushButton[accent=\"1\"]:checked {",
      "  background-color: {{ accent1_pressed }};",
      "  color: {{ accent1 }};",
      "}",
      "",
      "QCheckBox, QRadioButton {",
      "  padding-left: {{ padding_top }}px;",
      "}",
      "",
      "QRadioButton::indicator {",
      "  border-radius: 6px",
      "}",
      "",
      "QRadioButton {",
      "  border-radius: {{ round_radius }}px;",
      "}"
    ],
    "Containers": [
      "/* Tab Bar */",
      "QTabWidget::pane {",
      "  background-color: transparent;",
      "  border-radius: 2px;",
      "}",
      "",
      "QTabWidget>QFrame {",
      "  background-color: {{ background_layered }};",
      "}",
      "",
      "QTabBar::tab {",
      "  border: none;",
      "  padding: {{ padding }}px;",
      "}",
      "",
      "QTabBar::tab:top,",
      "QTabBar::tab:bottom {",
      "  padding-left: {{ padding_top }}px;",
      "  padding-right: {{ padding_top }}px;",
      "  height: 22;",
      "  margin-left: 1px;",
      "  margin-right: 1px;",
      "}",
      "",
      "QTabBar::tab:top {",
      "  border-top-left-radius: {{ radius }}px;",
      "  border-top-right-radius: {{ padding_top }}px;",
      "}",
      "",
      "QTabBar::tab:bottom {",
      "  border-bottom-left-radius: {{ padding_top }}px;",
      "  border-bottom-right-radius: {{ radius }}px;",
      "}",
      "",
      "QTabBar::tab:left,",
      "QTabBar::tab:right {",
      "  padding-top: {{ padding_top }}px;",
      "  padding-bottom: {{ padding_top }}px;",
      "  width: 22;",
      "  margin-top: 1px;",
      "  margin-bottom: 1px;",
      "}",
      "",
      "QTabBar::tab:left {",
      "    border-top-left-radius: {{ radius }}px;",
      "    border-bottom-left-radius: {{ padding_top }}px;",
      "}",
      "",
      "QTabBar::tab:right {",
      "  border-top-right-radius: {{ radius }}px;",
      "  border-bottom-right-radius: {{ padding_top }}px;",
      "}",
      "",
      "/* Group Box */",
      "QGroupBox {",
      "  margin-top: {{ padding_top }}px;",
      "  padding-top: {{ padding_top }}px;",
      "  background-color: {{ background_layered }};",
      "}",
      "",
      "QGroupBox::title {",
      "  top: -{{ padding_top }}px;",
      "  left: {{ padding_top }}px;",
      "}",
      "",
      "/* Splitter */",
      "QSplitter::handle {",
      "  background-color: {{ background_layered }};",
      "  margin: {{ padding }}px;",
      "}",
      "",
      "QSplitter>QFrame {",
      "  background-color: {{ background_layered }};",
      "}"
    ]
  },
