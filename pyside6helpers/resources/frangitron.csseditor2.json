{
  "version": 1,
  "templates": {
    "Main": [
      "* {",
      "  font: {{ font_size }}pt;",
      "  color: {{ foreground }};",
      "  background-color: transparent;",
      "  border-radius: {{ radius }}px;",
      "  outline: none;",
      "  padding: {{ padding }}px;",
      "}",
      "",
      "{{ interactives }} {",
      "  background-color: {{ background_layered }};",
      "}",
      "",
      "{{ interactives }}:focus {",
      "  border: {{ line_size }}px solid {{ foreground }};",
      "}",
      "",
      "",
      "{{ interactives }}:hover {",
      "  background-color: {{ hovered }};",
      "}",
      "",
      "QMainWindow, QFrame {",
      "  background-color: {{ background }};",
      "}",
      "",
      "QLabel {",
      "  background-color: transparent;",
      "}",
      "",
      "QPushButton {",
      "  background-color: {{ accent1 }};",
      "}",
      "",
      "QGroupBox {",
      "  background-color: {{ background_layered }};",
      "}",
      "",
      "QSlider:focus {",
      "  border-width: 0;",
      "}",
      "",
      "QTabBar::tab {",
      "    border: none;",
      "    padding: {{ padding }}px;",
      "}",
      "",
      "QToolBar  {",
      "    background-color: {{ background }};",
      "}"
    ]
  },
  "variables": [
    {
      "name": "background",
      "desktop_value": "[70, 70, 70]",
      "touch_value": ""
    },
    {
      "name": "foreground",
      "desktop_value": "[234, 234, 234]",
      "touch_value": ""
    },
    {
      "name": "accent1_hover",
      "desktop_value": "[71, 166, 255]",
      "touch_value": ""
    },
    {
      "name": "padding",
      "desktop_value": "2",
      "touch_value": "10"
    },
    {
      "name": "font_size",
      "desktop_value": "10",
      "touch_value": "12"
    },
    {
      "name": "background_layered",
      "desktop_value": "[255, 255, 255, 30]",
      "touch_value": ""
    },
    {
      "name": "radius",
      "desktop_value": "2",
      "touch_value": "4"
    },
    {
      "name": "line_size",
      "desktop_value": "1",
      "touch_value": "2"
    },
    {
      "name": "interactives",
      "desktop_value": "{QLineEdit, QTextEdit, QPlainTextEdit, QSpinBox, QDoubleSpinBox, QDateEdit, QTimeEdit, QDateTimeEdit, QComboBox, QPushButton, QCheckBox, QRadioButton, QTabBar::tab}",
      "touch_value": ""
    },
    {
      "name": "hovered",
      "desktop_value": "[153, 153, 153]",
      "touch_value": ""
    },
    {
      "name": "accent1",
      "desktop_value": "[59, 158, 229]",
      "touch_value": ""
    }
  ],
  "save_to": "E:\\PROJECTS_2023\\pyside6-helpers\\pyside6helpers\\resources\\dark-2.qss"
}