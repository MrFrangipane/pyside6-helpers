QPushButton, QLineEdit {
    color: rgb(232, 232, 232);
    background-color: rgb(104, 104, 104);
    border-radius: 2px;
    padding: 5px;
    padding-left: 10px;
    padding-right: 10px;
    height: 32px;
}

QPushButton:pressed {
    background-color: rgb(75, 165, 193);
    color: rgb(232, 232, 232);
}

QPushButton[primary=true] {
    background-color: rgb(75, 165, 193);
}

QPushButton:pressed:hover {
    background-color: rgb(106, 198, 237);
}

QPushButton[secondary=true] {
    background-color: transparent;
    border: 2px solid rgb(104, 104, 104);
    height: 30px;
}

QPushButton[secondary=true]:hover {
    background-color: rgb(104, 104, 104);
    border: 2px solid rgb(104, 104, 104);
    height: 30px;
}

QPushButton[secondary=true]:pressed {
    background-color: rgb(66, 66, 66);
}

QLineEdit:disabled,
QPushButton:disabled,
QPushButton[primary=true]:disabled {
    color: rgb(162, 162, 162);
    background-color: rgb(88, 88, 88);
    border-color: rgb(140, 140, 140);
    border-width: 1px;
    border-style: solid;
}
QCheckBox, QRadioButton {
    padding: 8px;
    padding-left: 10px;
}

QCheckBox:disabled,
QGroupBox::title:disabled,
QRadioButton:disabled {
    color: rgb(135, 135, 135);
}

QGroupBox::indicator::hover,
QCheckBox::hover,
QRadioButton::hover {
    background-color: rgb(117, 117, 117);
}

QRadioButton::hover {
    border-radius: 12px;
}

QCheckBox::indicator,
QGroupBox::indicator,
QRadioButton::indicator {
    border: 1px solid rgb(162, 162, 162);
    border-radius: 4px;
    width: 24px;
    height: 24px;
}

QCheckBox::indicator:disabled,
QGroupBox::indicator:disabled,
QRadioButton::indicator:disabled {
    border: 1px solid rgb(116, 116, 116);
    border-radius: 4px;
}

QCheckBox::indicator:checked,
QGroupBox::indicator:checked,
QRadioButton::indicator:checked {
    border: 1px solid rgb(75, 165, 193);
    background-color: rgb(75, 165, 193);
}

QCheckBox::indicator:checked:disabled,
QGroupBox::indicator:checked:disabled,
QRadioButton::indicator:checked:disabled {
    border: 1px solid rgb(116, 116, 116);
    background-color: rgb(116, 116, 116);
}

QCheckBox::indicator:checked:hover,
QGroupBox::indicator:checked:hover,
QRadioButton::indicator:checked:hover {
    background-color: rgb(106, 198, 237);
}

QRadioButton::indicator,
QRadioButton::indicator:disabled {
   border-radius: 12px;
}
QComboBox {
    background-color: rgb(104, 104, 104);
    selection-color: rgb(232, 232, 232);
    selection-background-color: rgb(75, 165, 193);
    padding: 10px;
    combobox-popup: 0;
    border-bottom-right-radius: 6px;
    height: 24px;
}

QComboBox::drop-down {
    background-color: rgb(140, 140, 140);
    border-bottom-left-radius: 6px;
    border-bottom-right-radius: 6px;
    width: 30px;
}

QComboBox::drop-down:hover {
    background-color: rgb(140, 140, 140);
}

QComboBox::hover {
    background-color: rgb(117, 117, 117);
}

QComboBox > QListView::item {
    height: 44px;
    border: 1px solid white;
}
* {
    font: 10pt;
    background: transparent;
    color: rgb(232, 232, 232);
    selection-color: rgb(232, 232, 232);
    selection-background-color: rgb(75, 165, 193);
    border-radius: 2px;
    outline: none;
}

QLineEdit:focus, QPushButton:focus , QSpinBox:focus {
    border-bottom: 2px solid rgb(232, 232, 232);
    border-bottom-left-radius: 0px;
    border-bottom-right-radius: 0px;
}

QPlainTextEdit:focus {
    border-left: 2px solid rgb(232, 232, 232);
    border-top-left-radius: 0px;
    border-bottom-left-radius: 0px;
}

QLineEdit:hover,
QPushButton:hover,
QHeaderView::section:hover,
QSpinBox:hover {
    background-color: rgb(117, 117, 117);
    color: rgb(232, 232, 232);
}

QPushButton:checked {
    background-color: rgb(75, 165, 193);
}

QPushButton:disabled:checked {
    background-color: rgb(140, 140, 140);
}

QPlainTextEdit:active,
QPlainTextEdit:!active  {
    background-color: rgba(255, 255, 255, 15);
}

QPlainTextEdit[readOnly="true"],
QPlainTextEdit[readOnly="true"]:focus {
    border: None;
}

QToolTip {
    color: rgb(232, 232, 232);
    border: 1px solid rgb(117, 117, 117);
    padding: 5px;
    background-color: rgb(66, 66, 66);
}
QDialog {
    background-color: rgb(66, 66, 66);
}

QDialog > QLabel {
    background-color: rgb(66, 66, 66);
}
QDockWidget {
    background: rgb(66, 66, 66);
}

QDockWidget::title {
    background: rgb(66, 66, 66);
    border-bottom: 1px solid rgb(140, 140, 140);
    border-bottom-left-radius: 0px;
    border-bottom-right-radius: 0px;
    margin-top: 5px;
    margin-right: 50px;
    padding: 5px;
}

QDockWidget::close-button, QDockWidget::float-button {
    color: rgb(232, 232, 232);
    background: rgb(104, 104, 104);
    margin: 6px;
    border-radius: 4px;
    width: 24px;
    height: 24px;
}

QDockWidget::close-button:hover, QDockWidget::float-button:hover {
    color: rgb(232, 232, 232);
    background: rgb(117, 117, 117);
}

QDockWidget::close-button:pressed, QDockWidget::float-button:pressed {
    background: rgb(75, 165, 193);
}
QFrame {
    background-color: rgb(66, 66, 66);
}
QGroupBox {
  background-color: rgba(255, 255, 255, 15);
  border: none;
  margin-top: 15px;
  padding-top: 12px;
}

QGroupBox::title {
    top: -12px;
    left: 10px;
    color: rgb(232, 232, 232);
    background: transparent;
    padding: 5px;
}
QLabel {
    background-color: transparent;
    color: rgb(185, 185, 185);
    padding: 5px;
}

QLabel:disabled {
    color: rgb(135, 135, 135);
}

QLabel[isWhite="true"] {
    color: rgb(232, 232, 232);
}
QMainWindow {
    background-color: rgb(66, 66, 66);
}
QMenu {
    background-color: rgb(66, 66, 66);
    padding: 5px;
    border: 1px solid rgb(104, 104, 104);
    border-radius: 0px;
}

QMenu::item {
    color: rgb(232, 232, 232);
    background-color: transparent;
    padding: 10px 25px 10px 20px;
}

QMenu::item:selected {
    background-color: rgb(104, 104, 104);
}

QMenu::item:checked {
    background-color: rgb(75, 165, 193);
}

QMenu::item:checked:selected {
    background-color: rgb(106, 198, 237);
}


QMenu::item:disabled {
    background-color: rgba(255, 255, 255, 15);
    color: rgb(140, 140, 140);
}


QMenu::separator {
    height: 2px;
    background: rgb(104, 104, 104);
    margin: 5px 10px;
}


QMenuBar {
    background-color: rgba(255, 255, 255, 15);
    spacing: 7px;
}

QMenuBar:disabled {
    color:  rgb(135, 135, 135);
}

QMenuBar::item {
    padding: 10px 15px;
    border-radius: 0px;
}

QMenuBar::item:selected {
    background-color: rgb(75, 165, 193);
    padding: 10px 15px;
    border-radius: 0px;
}

QMenuBar::item:pressed {
    background: rgb(140, 140, 140);
}
QProgressBar {
    border: none;
    background-color: rgb(104, 104, 104);
    padding: 0;
    border-radius: 4px;
}

QProgressBar:horizontal {
    margin-top: 10px;
    margin-bottom: 10px;
    height: 12px;
}

QProgressBar:vertical {
    width: 12px;
    margin-left: 10px;
    margin-right: 10px;
}

QProgressBar::chunk {
    background-color: rgb(75, 165, 193);
    border-radius: 4px;
}
QLabel[isTitle="true"] {
    font-size: 13pt;
}

*[sideStroked="true"], QLabel[isTitle="true"] {
    border-style: solid;
    border-color: rgb(75, 165, 193);
    border-left-width: 7px;
    border-radius: 0px;
}

QGroupBox[untitledGroup="true"] {
      padding-top: 0;
}
QGraphicsView {
    background: rgba(255, 255, 255, 15);
}
QScrollArea, *[hasBackground="true"] {
    background-color: transparent;
    border-radius: 2px;
}

QScrollBar {
    background-color: rgb(140, 140, 140);
    border-radius: 4px;
    border: none;
}

QScrollBar:vertical {
    width: 25px;
    margin: 0px;
}

QScrollBar:horizontal {
    height: 25px;
    margin: 0px;
}

QScrollBar::add-page, QScrollBar::sub-page {
    border-radius: 4px;
    background: none;
}

QScrollBar::handle {
    background-color: rgb(75, 165, 193);
    min-height: 40px;
    min-width: 40px;
    border-radius: 4px;
}

QScrollBar::add-line, QScrollBar::sub-line {
    border: none;
    background: none;
    width: 0px;
    height: 0px;
}

QScrollBar::handle:hover {
    background-color: rgb(106, 198, 237);
}

QScrollBar::handle:disabled {
    background-color: rgb(185, 185, 185);
}
QSlider:horizontal {
    height: 35px;
}

QSlider:vertical{
    width: 35px;
}

QSlider::groove {
    background: rgba(255, 255, 255, 15);
    position: absolute;
}

QSlider::groove:focus {
    background: rgb(104, 104, 104);
}

QSlider::groove:horizontal {
    left: 12px;
    right: 12px;
}

QSlider::groove:vertical {
    top: 12px;
    bottom: 12px;
}

QSlider::handle {
    background: rgb(75, 165, 193);
    border-radius: 12px;
}

QSlider::handle:hover {
    background: rgb(106, 198, 237);
}

QSlider::handle:horizontal {
    margin: -10px -12px;
    width: 24px;
    height: 24px;
}

QSlider::handle:vertical {
    margin: -12px -10px;
    width: 24px;
    height: 24px;
}

QSlider::groove:horizontal {
    height: 10px;
}

QSlider::groove:vertical {
    width: 10px;
}
QDoubleSpinBox,
QSpinBox {
    height: 35px;
    background-color: rgb(104, 104, 104);
    padding-left: 8px;
    border-top-right-radius: 6px;
    border-bottom-right-radius: 6px;
    border: 2px solid rgb(104, 104, 104);
}

QDoubleSpinBox:disabled,
QSpinBox:disabled {
    color: rgb(135, 135, 135);
}

QDoubleSpinBox::up-button, QDoubleSpinBox::down-button,
QSpinBox::up-button, QSpinBox::down-button {
    background-color: rgb(140, 140, 140);
    width: 30px;
    height: 16px;
    margin: 1px;
}

QDoubleSpinBox::up-button,
QSpinBox::up-button {
    border-top-left-radius: 6px;
    border-top-right-radius: 6px;
}

QDoubleSpinBox::down-button,
QSpinBox::down-button {
    border-bottom-left-radius: 6px;
    border-bottom-right-radius: 6px;
}

QDoubleSpinBox::up-button:hover, QDoubleSpinBox::down-button:hover,
QSpinBox::up-button:hover, QSpinBox::down-button:hover {
    background-color: rgb(117, 117, 117);
}

QDoubleSpinBox::up-button:pressed, QDoubleSpinBox::down-button:pressed,
QSpinBox::up-button:pressed, QSpinBox::down-button:pressed {
    background-color: rgb(75, 165, 193);
}
QStatusBar {
    background-color: rgba(255, 255, 255, 15);
}


QStatusBar > QLabel {
    color: rgb(185, 185, 185);
    padding: 5px;
}

QStatusBar::item {
    border: none;
}
QTabWidget::pane {
    background-color: transparent;
    border-radius: 2px;
}

QTabBar {
    background-color: transparent;
}

QTabBar::tab {
    color: rgb(232, 232, 232);
    background-color: rgb(104, 104, 104);
    padding: 5px;
}

QTabBar::tab:top,
QTabBar::tab:bottom {
    padding-left: 15px;
    padding-right: 15px;
    height: 40px;
    margin-left: 2px;
    margin-right: 2px;
}

QTabBar::tab:top {
    border-top-left-radius: 4px;
    border-top-right-radius: 8px;
}

QTabBar::tab:bottom {
    border-bottom-left-radius: 8px;
    border-bottom-right-radius: 4px;
}

QTabBar::tab:left,
QTabBar::tab:right {
    padding-top: 15px;
    padding-bottom: 15px;
    width: 40px;
    margin-top: 2px;
    margin-bottom: 2px;
}

QTabBar::tab:left {
    border-top-left-radius: 4px;
    border-bottom-left-radius: 8px;
}

QTabBar::tab:right {
    border-top-right-radius: 4px;
    border-bottom-right-radius: 8px;
}

QTabBar::tab:hover {
    background-color: rgb(117, 117, 117);
    color: rgb(232, 232, 232);
}

QTabBar::tab:pressed {
    background-color: rgb(75, 165, 193);
    color: rgb(232, 232, 232);
}

QTabBar::tab:selected {
    background-color: rgb(75, 165, 193);
    color: rgb(232, 232, 232);
}

QTabBar::tab:selected:left,
QTabBar::tab:selected:right {
    border-bottom: 3px solid rgb(232, 232, 232);
    margin-top: 1px;
}

QTabBar::tab:selected:right {
    border-bottom-right-radius: 2px;
}

QTabBar::tab:selected:left {
    border-bottom-left-radius: 2px;
}

QTabBar::tab:selected:top,
QTabBar::tab:selected:bottom {
    border-left: 3px solid rgb(232, 232, 232);
    margin-left: 1px;
}

QTabBar::tab:selected:top {
    border-top-right-radius: 2px;
}

QTabBar::tab:selected:bottom {
    border-bottom-left-radius: 2px;
}

QTabBar::tab:disabled {
    color: rgb(139, 139, 139);
}

QTabBar::tab:selected:disabled {
    background-color: rgb(140, 140, 140);
    color: rgb(92, 92, 92);
}
QAbstractItemView {
    color: rgb(232, 232, 232);
    background-color: rgb(104, 104, 104);
    alternate-background-color: rgb(93, 93, 93);
}

QAbstractItemView > QWidget > QLineEdit,
QAbstractItemView > QWidget > QLineEdit:hover {
    background: rgb(75, 165, 193);
    border: none;
    border-bottom: 2px solid rgb(232, 232, 232);
    border-radius: 0px;
    selection-color: rgb(75, 165, 193);
    selection-background-color: rgb(232, 232, 232);
}

QAbstractItemView::item {
    padding: 8px;
}

QAbstractItemView::item:focus,
QAbstractItemView::item:selected
{
    border: none;
    padding-left: 10px;
}

QAbstractItemView::item:focus
{
    border-bottom: 2px solid rgb(232, 232, 232);
    padding-bottom: 6px;
}

QListView {
    background-color: rgb(140, 140, 140);
}

QWidget > QListView {
    background: transparent;
}

QListView::item:focus
{
    background: transparent;
    border: none;
}

QAbstractItemView::item:selected:active {
    color: rgb(232, 232, 232);
    background-color: rgb(75, 165, 193);
}

QAbstractItemView::item:selected:!active {
    color: rgb(66, 66, 66);
    background-color: rgb(135, 135, 135);
}

QHeaderView::section {
    color: rgb(232, 232, 232);
    background-color: rgb(66, 66, 66);
    border-color: rgb(66, 66, 66);
    border-style: solid;
    border-left-width: 1px;
    border-right-width: 1px;
    border-top-width: 1px;
    border-bottom-width: 1px;
    padding: 10px;
}

QAbstractItemView QTableCornerButton::section {
    border-color: rgb(66, 66, 66);
    border-style: solid;
    border-left-width: 1px;
    border-right-width: 1px;
    border-top-width: 1px;
    border-bottom-width: 1px;
    background-color: rgb(66, 66, 66);
}

QHeaderView::section:hover {
    background-color: rgb(117, 117, 117);
    color: rgb(232, 232, 232);
}


QAbstractItemView:disabled,
QHeaderView::section:disabled {
    color: rgb(162, 162, 162);
}
QToolBar  {
    background-color: rgba(255, 255, 255, 15);
    padding: 6px;
}

QToolBar::separator {
    background-color: rgb(140, 140, 140);
    margin: 5px;
}

QToolButton, QToolButton:unchecked {
    background: transparent;
    border-radius: 4px;
    padding: 8px;
}

QToolButton:hover {
    background-color: rgb(117, 117, 117)
}

QToolButton:pressed {
    background-color: rgb(75, 165, 193);
}

QToolButton:checked {
    background-color: rgb(75, 165, 193);
}

QToolButton:checked:hover {
    background-color: rgb(106, 198, 237);
}

QToolBar::handle {
    background-color: rgba(255, 255, 255, 15);
}

QToolBar::handle:horizontal {
    margin-right: 2px;
}

QToolBar::handle:vertical {
    margin-bottom: 2px;
}